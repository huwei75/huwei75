public Parent(String name, int age) {
        System.out.println("Parent constructor method");
        this.name = "lishi";
        this.age = 51;
    }

    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }

    public int getAge() {
        return age;
    }

    public void setAge(int age) {
        this.age = age;
    }
}
